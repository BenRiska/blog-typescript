[{"/Users/Ben/Desktop/blog-ts/src/index.tsx":"1","/Users/Ben/Desktop/blog-ts/src/reportWebVitals.ts":"2","/Users/Ben/Desktop/blog-ts/src/App.tsx":"3","/Users/Ben/Desktop/blog-ts/src/components/Landing.tsx":"4","/Users/Ben/Desktop/blog-ts/src/cursor.js":"5","/Users/Ben/Desktop/blog-ts/src/utils.ts":"6","/Users/Ben/Desktop/blog-ts/src/animations.ts":"7","/Users/Ben/Desktop/blog-ts/src/components/BlogList.tsx":"8","/Users/Ben/Desktop/blog-ts/src/blogTitles.js":"9","/Users/Ben/Desktop/blog-ts/src/components/Grid.tsx":"10","/Users/Ben/Desktop/blog-ts/src/components/List.tsx":"11","/Users/Ben/Desktop/blog-ts/src/components/Blog.tsx":"12","/Users/Ben/Desktop/blog-ts/src/blogs.tsx":"13","/Users/Ben/Desktop/blog/blog-typescript/src/index.tsx":"14","/Users/Ben/Desktop/blog/blog-typescript/src/reportWebVitals.ts":"15","/Users/Ben/Desktop/blog/blog-typescript/src/App.tsx":"16","/Users/Ben/Desktop/blog/blog-typescript/src/cursor.js":"17","/Users/Ben/Desktop/blog/blog-typescript/src/animations.ts":"18","/Users/Ben/Desktop/blog/blog-typescript/src/components/Landing.tsx":"19","/Users/Ben/Desktop/blog/blog-typescript/src/components/BlogList.tsx":"20","/Users/Ben/Desktop/blog/blog-typescript/src/components/Blog.tsx":"21","/Users/Ben/Desktop/blog/blog-typescript/src/blogTitles.js":"22","/Users/Ben/Desktop/blog/blog-typescript/src/utils.ts":"23","/Users/Ben/Desktop/blog/blog-typescript/src/components/Grid.tsx":"24","/Users/Ben/Desktop/blog/blog-typescript/src/components/List.tsx":"25","/Users/Ben/Desktop/blog/blog-typescript/src/blogs.tsx":"26"},{"size":587,"mtime":1606901509220,"results":"27","hashOfConfig":"28"},{"size":425,"mtime":1606833572465,"results":"29","hashOfConfig":"28"},{"size":4375,"mtime":1607618920382,"results":"30","hashOfConfig":"28"},{"size":1091,"mtime":1606848688814,"results":"31","hashOfConfig":"28"},{"size":2297,"mtime":1606838803409,"results":"32","hashOfConfig":"28"},{"size":895,"mtime":1606837530416,"results":"33","hashOfConfig":"28"},{"size":2571,"mtime":160